<?xml version="1.0" encoding="UTF-8"?>
<facelet-taglib
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
    version="2.0">
	<namespace>http://timetime.com/functions</namespace>
	<function>
		<function-name>concat</function-name>
		<function-class>com.approachingpi.timetime.view.tags.Functions</function-class>
		<function-signature>
			java.lang.String concat(java.lang.String, java.lang.String)
		</function-signature>
	</function>

	<!--
	concat3 was created because I can't figure out how to do a custom tag
	with support for varargs. If this is possible we should switch to a varargs implementation -->
	<function>
		<function-name>concat3</function-name>
		<function-class>com.approachingpi.timetime.view.tags.Functions</function-class>
		<function-signature>
			java.lang.String concat(java.lang.String, java.lang.String, java.lang.String)
		</function-signature>
	</function>

	<function>
		<function-name>sort</function-name>
		<function-class>com.approachingpi.timetime.view.tags.Functions</function-class>
		<function-signature>
			java.util.List sort(java.util.List, java.lang.String)
		</function-signature>
	</function>

	<function>
		<function-name>phoneFormat</function-name>
		<function-class>com.approachingpi.timetime.view.tags.Functions</function-class>
		<function-signature>
			java.lang.String phoneFormat(java.lang.String)
		</function-signature>
	</function>

</facelet-taglib>